{
  "name": "ember-easy-orm",
  "description": "A easy ORM library for ember app",
  "version": "1.0.0",
  "options": {
    "paths": [
      "addon"
    ],
    "enabledEnvironments": ["development", "production"],
    "exclude": "vendor",
    "outdir": "docs",
    "linkNatives": true,
    "quiet": true,
    "parseOnly": false,
    "lint": false,
    "themedir" : "node_modules/yuidoc-lucid-theme",
    "helpers" : ["node_modules/yuidoc-lucid-theme/helpers/helpers.js"]
  }
}